{
  "name": "go-imports",
  "displayName": "go-imports",
  "description": "",
  "version": "0.0.1",
  "publisher": "aleksandra",
  "engines": {
    "vscode": "^1.20.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onLanguage:go",
    "onCommand:extension.goGroupImports"
  ],
  "main": "./out/extension",
  "contributes": {
    "commands": [
      {
        "command": "extension.goGroupImports",
        "title": "Group imports"
      }
    ]
  },
  "configuration": {
    "title": "Group Imports configuration",
    "properties": {
      "groupImports.onSave": {
        "type": "boolean",
        "default": true,
        "description": "Automatically group imports on save."
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install",
    "test": "npm run compile && node ./node_modules/vscode/bin/test"
  },
  "devDependencies": {
    "@types/mocha": "^2.2.42",
    "@types/node": "^7.0.43",
    "typescript": "^3.1.2",
    "vscode": "^1.1.21"
  }
}
